all:	squashfsbroadcom squashfsralink squashfsopenwrtold cramfsunpack unyaffsunpack

squashfsbroadcom:
	cd squashfs-broadcom; make

squashfsralink:
	cd squashfs-ralink; make

squashfsopenwrtold:
	cd squashfs-openwrt; make

squashfs42:
	cd squashfs4.2/squashfs-tools; make

cramfsunpack:
	cd cramfs; ./configure; make

unyaffsunpack:
	cd unyaffs; make
